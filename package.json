{
  "name": "cypress-training",
  "version": "1.0.0",
  "description": "This is a Workshop about Cypress",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./cypress/integration/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "test:open": "npm run lint && cypress open",
    "clean:reports": "rm -rf cypress\\reports 2> nul && mkdir cypress\\reports && mkdir cypress\\reports\\mocha",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "generate-report": "marge cypress/reports/report.json -f report -o cypress/reports",
    "pretest": "npm run clean:reports",
    "test": "npm run lint && cypress run",
    "posttest": "npm run combine-reports && npm run generate-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgileTestingColombia/cypress-training.git"
  },
  "keywords": [
    "ui-tesing",
    "cypress",
    "workshop"
  ],
  "author": "Software Developer Engineer in Test <ylopezs@psl.com.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgileTestingColombia/cypress-training/issues"
  },
  "homepage": "https://github.com/AgileTestingColombia/cypress-training#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cypress": "^4.11.0",
    "cypress-iframe": "^1.0.1",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "mocha": "^8.3.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6"
  }
}
